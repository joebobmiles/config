  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "author": "Joseph R Miles <joe@jrm.dev> (https://jrm.dev)",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/joebobmiles/eslint-config/issues"
  },
  "homepage": "https://github.com/joebobmiles/eslint-config#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "husky": "^4.3.6",
    "semantic-release": "^17.3.1"
  },
  "main": "eslintrc.json",
  "files": [
    "eslintrc.json",
    "package.json"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
